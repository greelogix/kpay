{
    "name": "greelogix/kpay-laravel",
    "description": "KPay - Laravel package for KNET payment gateway integration with payment processing, callbacks, and refunds.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "knet",
        "payment",
        "gateway",
        "payment-processing",
        "kuwait",
        "middle-east",
        "kfast",
        "apple-pay"
    ],
    "homepage": "https://github.com/greelogix/kpayment",
    "support": {
        "email": "asad.ali@greelogix.com",
        "issues": "https://github.com/greelogix/kpayment/issues",
        "source": "https://github.com/greelogix/kpayment"
    },
    "authors": [
        {
            "name": "Asad ALi",
            "email": "asad.ali@greelogix.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Greelogix\\KPay\\": "src/",
            "Greelogix\\KPay\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greelogix\\KPay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Greelogix\\KPay\\KPayServiceProvider"
            ],
            "aliases": {
                "KPay": "Greelogix\\KPay\\Facades\\KPay"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}



